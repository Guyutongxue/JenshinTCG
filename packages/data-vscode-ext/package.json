    "build": "bun run build:method_names && bun run build:esbuild",
    "watch": "bun esbuild.js --watch",
    "package": "bun run build",
    "check": "tsc --noEmit"
  },
  "dependencies": {
    "typescript": "^5.6.2",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@gi-tcg/core": "workspace:*",
    "@types/vscode": "^1.96.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "esbuild": "^0.24.0",
    "npm-run-all": "^4.1.5"
  }
}
